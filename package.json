{
  "name": "@uvdsl/solid-access-requests",
  "version": "0.0.1",
  "author": "uvdsl - Christoph Braun",
  "license": "MIT",
  "description": "A lightweight TypeScript library for generating Solid Access Requests.",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/uvdsl/solid-access-requests.git"
  },
  "scripts": {
    "compile": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.types.json",
    "prebuild": "rimraf ./dist",
    "build": "run-s compile",
    "test": "jest --coverage",
    "postbuild": "node ./scripts/prepare-package-json.js"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "jest": "^29.0.0",
    "npm-run-all2": "^7.0.1",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.0.0",
    "typescript": "^5.7.2"
  },
  "dependencies": {}
}